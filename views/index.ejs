</div>
<%blogs.forEach(function(blog){%>

<div class="eachblog">
    <style>.eachblog{ margin-left:10px;}</style>
    <h2><%=blog.title%></h2>
    <img class ="ui small circular image"src="<%=blog.image%>">
    <span><%=blog.created%></span>
    <p><%=blog.body.substring(0,100)%></p>
    <a href="/blogs/<%=blog.id%>"> read more</a>
</div>

<% })%>

